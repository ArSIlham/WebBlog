<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication16</title>
    <link rel="stylesheet" href="~/font/iconsmind-s/css/iconsminds.css" />
    <link rel="stylesheet" href="~/font/simple-line-icons/css/simple-line-icons.css" />

    <link rel="stylesheet" href="~/css/vendor/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/vendor/bootstrap.rtl.only.min.css" />
    <link rel="stylesheet" href="~/css/vendor/fullcalendar.min.css" />
    <link rel="stylesheet" href="~/css/vendor/dataTables.bootstrap4.min.css" />
    <link rel="stylesheet" href="~/css/vendor/datatables.responsive.bootstrap4.min.css" />
    <link rel="stylesheet" href="~/css/vendor/select2.min.css" />
    <link rel="stylesheet" href="~/css/vendor/perfect-scrollbar.css" />
    <link rel="stylesheet" href="~/css/vendor/glide.core.min.css" />
    <link rel="stylesheet" href="~/css/vendor/bootstrap-stars.css" />
    <link rel="stylesheet" href="~/css/vendor/nouislider.min.css" />
    <link rel="stylesheet" href="~/css/vendor/bootstrap-datepicker3.min.css" />
    <link rel="stylesheet" href="~/css/vendor/component-custom-switch.min.css" />
    <link rel="stylesheet" href="~/css/vendor/quill.snow.css" />
    <link rel="stylesheet" href="~/css/vendor/quill.bubble.css" />
    <link rel="stylesheet" href="~/css/main.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">WebApplication16</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - WebApplication16 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/js/vendor/jquery-3.3.1.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/bootstrap.bundle.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/Chart.bundle.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/chartjs-plugin-datalabels.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/moment.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/fullcalendar.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/datatables.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/perfect-scrollbar.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/progressbar.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/jquery.barrating.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/select2.full.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/nouislider.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/bootstrap-datepicker.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/Sortable.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/mousetrap.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/glide.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/ckeditor5-build-classic/ckeditor.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/vendor/quill.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/dore.script.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/scripts.js" asp-append-version="true" type="text/javascript"></script>
    @RenderSection("Scripts", required: false)

</body>
</html>
